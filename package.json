{
  "name": "refreshify-allnotifications",
  "version": "0.0.3",
  "description": "alexandergugel's refreshify + option to enable notifications for nonerroring builds + customizable notification title",
  "scripts": {
    "test": "tape test/**/*.js",
    "example": "node bin/cmd.js ./example/index.js -d -o example/index.bundle.js -v"
  },
  "author": "Corin Chellberg (corinchellberg@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "tape": "^4.0.0"
  },
  "dependencies": {
    "browserify": "^9.0.7",
    "node-notifier": "^4.1.2",
    "watchify": "^3.1.0"
  },
  "bin": {
    "refreshify-allnotifications": "bin/cmd.js"
  },
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chellberg/refreshify-allnotifications.git"
  },
  "bugs": {
